def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        res = list()
        for i in range(len(nums)):
            for j in range(i+1,len(nums)):
                if nums[i]+nums[j]==target:
                    res.append(i)
                    res.append(j)

        return res

# Example usage:
nums = [2, 7, 11, 15]
target = 9
print(twoSum(None, nums, target))  # Output: [0, 1]
