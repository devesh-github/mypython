maxn = lambda a, b: a if a > b else b
class MaxHopScore:
    from typing import List
    from builtins import max as maxn

    
    def maxScore(self, nums: List[int]) -> int:
            ans = 0
            mx = 0
            for num in nums[-1:0:-1]:
                mx = maxn(mx, num)
                ans+=mx
            return ans

print(MaxHopScore().maxScore([4,5,2,8,9,1,3]))  # Example usage